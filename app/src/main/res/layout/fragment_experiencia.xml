<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.FragmentMenuInicio.ExperienciaFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Culturas"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="5dp"
        android:text="Cultura viva"
        android:textSize="16sp"
        android:textStyle="bold|italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Milenarios"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="44dp"
        android:layout_marginRight="44dp"
        android:text="Milenario"
        android:textSize="16sp"
        android:textStyle="bold|italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Aventuras"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="54dp"
        android:layout_marginLeft="54dp"
        android:layout_marginTop="1dp"
        android:text="Aventura"
        android:textSize="16sp"
        android:textStyle="bold|italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Naturals"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="51dp"
        android:layout_marginRight="51dp"
        android:text="Natural"
        android:textSize="16sp"
        android:textStyle="bold|italic" />

    <ImageButton
        android:id="@+id/Culturas"
        android:layout_width="84dp"
        android:layout_height="83dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="88dp"
        android:background="@drawable/circulo"
        android:src="@android:drawable/star_big_on"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="2dp" />

    <ImageButton
        android:id="@+id/Milenarios"
        android:layout_width="84dp"
        android:layout_height="83dp"
        android:layout_alignTop="@+id/Culturas"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="-3dp"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="41dp"
        android:background="@drawable/circulo"
        android:src="@android:drawable/star_big_on" />

    <ImageButton
        android:id="@+id/Aventuras"
        android:layout_width="84dp"
        android:layout_height="83dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="325dp"
        android:background="@drawable/circulo"
        android:src="@android:drawable/star_big_on" />

    <ImageButton
        android:id="@+id/Naturals"
        android:layout_width="84dp"
        android:layout_height="83dp"
        android:layout_alignTop="@+id/Aventuras"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="39dp"
        android:layout_marginRight="39dp"
        android:background="@drawable/circulo"
        android:src="@android:drawable/star_big_on" />

</RelativeLayout>